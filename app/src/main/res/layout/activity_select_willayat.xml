<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
    android:id="@+id/container_root_SelectWillayat"
tools:context="com.gpd.gpdimg.bin.SelectWillayat">

<ImageView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/header_oges_SelectWillayat"
    android:scaleType="centerCrop"
    android:src="@drawable/bg_image2"/>

    <RelativeLayout android:id="@+id/header_oges_SelectWillayat"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:background="@color/white">
        <ImageView
            android:layout_width="200dp"
            android:layout_height="90dp"
            android:layout_centerInParent="true"
            android:id="@+id/img_logo"
            android:visibility="gone"
            android:src="@drawable/ic_logo_company"/>
    </RelativeLayout>

    <ImageView android:id="@+id/iv_slanting_edge_SelectWillayat"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:layout_marginTop="-12dp"
        android:layout_below="@+id/header_oges_SelectWillayat"
        android:src="@drawable/iv_slanting"/>

    <LinearLayout android:id="@+id/container_selected_values_SelectWillayat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        android:layout_below="@+id/iv_slanting_edge_SelectWillayat"
        android:orientation="horizontal">

        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:flexWrap="wrap"
            app:alignItems="stretch"
            app:alignContent="stretch" >
        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/ic_location_black"/>
        <TextView android:id="@+id/tv_selected_governorate_value_SelectWillayat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:layout_marginLeft="2dp"
            android:textSize="12sp"
            android:src="@drawable/ic_down_arrow_spinner"/>
          </com.google.android.flexbox.FlexboxLayout>
    </LinearLayout>


    <RelativeLayout  android:id="@+id/container_show_value_SelectWillayat"
        android:layout_width="match_parent"
        android:layout_height="46dp"
        android:background="@drawable/spinner_bg"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="66dp"
        android:layout_marginBottom="36dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/container_selected_values_SelectWillayat">

        <TextView android:id="@+id/tv_selected_willayat_from_list_SelectWillayat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="18sp"
            android:textColor="@color/white"
            android:text="Select Willayat"/>

    </RelativeLayout>


    <Spinner android:id="@+id/sp_SelectWillayat"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:visibility="gone"
    android:background="@drawable/spinner_bg"
    android:layout_below="@+id/container_selected_values_SelectWillayat"
    android:layout_marginLeft="36dp"
    android:layout_marginRight="36dp"
    android:layout_marginTop="56dp"
    android:layout_marginBottom="36dp"
    android:layout_centerHorizontal="true"/>

<RelativeLayout android:id="@+id/rl_enter_SelectWillayat"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentRight="true"
    android:padding="6dp"
    android:layout_marginRight="36dp"
    android:background="@drawable/blue_button_bg"
    android:layout_below="@+id/container_show_value_SelectWillayat">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="36dp"
        android:layout_marginRight="36dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:textColor="@drawable/blue_button_text_click"
        android:textSize="14sp"
        android:text="ENTER"/>
</RelativeLayout>


</RelativeLayout>
